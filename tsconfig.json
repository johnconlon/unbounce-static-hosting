{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "jsx": "react",
    "jsxFactory": "h",
    "sourceMap": true
  },
  "include": ["src/**/*", "src/node_modules/**/*"]
}
